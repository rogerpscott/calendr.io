<div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('<%= image_path 'tennis.jpg' %>');">
  <div class="banner-content">
    <h1>Calendr.io</h1>
    <p>The easiest way to manage calendars for <span class="element"></span></p>
    <%= link_to 'All bookings', bookings_path, class:'btn btn-primary' %>
    <%= link_to 'All my places', places_path, class:'btn btn-success' %>


  </div>
</div>

<% content_for(:after_js) do %>
  <%= javascript_tag do %>
    $(document).ready(function(){
        $(".element").typed({
            strings: ["^3000 meeting rooms", "^2000 tennis courts", "^2000 the family beach house", "^2000 ..."],
            typeSpeed: 0
        });
    });
  <% end %>
<% end %>
